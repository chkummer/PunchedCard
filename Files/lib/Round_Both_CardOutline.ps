% Outline of a punched card where both upper corners are cut
/cardOutline
 { newpath
   @@CUT_OUTLINE_COLOR@@ setrgbcolor
   0.25 setlinewidth
   18 0 moveto
   495 0 rlineto
   513 18 18 270 360 arc
   0 184.824 rlineto
   -18 31.176 rlineto
   -495  0 rlineto
   -18 -31.176 rlineto
   0 -184.824 rlineto
   18 18 18 180 270 arc
   closepath
   stroke } def
