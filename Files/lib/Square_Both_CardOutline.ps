% Outline of a punched card where both upper corners are cut
/cardOutline
 { newpath
   @@CUT_OUTLINE_COLOR@@ setrgbcolor
   0.25 setlinewidth
   0 0 moveto
   531 0 rlineto
   0 202.824 rlineto
   -18 31.176 rlineto
   -495  0 rlineto
   -18 -31.176 rlineto
   0 -202.824 rlineto
   closepath
   stroke } def
