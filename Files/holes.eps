% holes.eps

/Pos 0 def
/Hole 0 def

/centreshow 
  { /mystring exch def
    mystring stringwidth pop -2 div 0 rmoveto 
    mystring show } def 

/hole
 { newpath moveto
   0.25 setlinewidth
   -1.98 -4.5 rmoveto
   3.96 0 rlineto
   0 9 rlineto
   -3.96 0 rlineto
   0 -9 rlineto
   closepath
   stroke } def

/punchHole
 { /XPos Pos 6.264 mul 11.808 add def
   XPos [180 162 144 126 108 90 72 54 36 18 0 198 218] Hole get hole } def

/testHoles
 { /Pos 1 def
   { [9 8 7 6 5 4 3 2 1 0 11 12] { /Hole exch def punchHole} forall 
     /Pos Pos 1 add def
     Pos 80 gt
     { exit } if
   } loop } def
 
20 40 translate
testHoles

showpage
